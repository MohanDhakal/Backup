abstract class Animal
{
  abstract void makeSound();
 
}
class Cat extends Animal
{
  public void makeSound(){
    System.out.println("Mew");
  }
}
class Dog extends Animal
{
  public void makeSound()
  {
      System.out.println("Mew");
  }
}
class Check
{
  public static void main(String args[]){
    Animal ob=new Cat();
    ob.makeSound();
    Dog d=new Animal();
    d.makeSound();
  }
}
  
  