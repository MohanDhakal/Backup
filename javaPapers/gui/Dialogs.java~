import java.swing.*;

class Dialogs
{
  JButton b1,b2;
  JTextField t1;
  Dialogs()
  {
    JFrame frame=new JFrame();
    frame.setLayout(new FlowLayout());
    
     b1=new JButton("sum");
    frame.add(b1);
    
     t1=new JTextField(10);
    frame.add(t1);
    
    b2=new JButton("delete");
    frame.add(b2);
    
    frame.setVisible(true);
    frame.setSize(200,200);
  }
  public static void main(String args[])
  {
    new Dialogs();
  }
}
    
    