class NUpCount
{
  static int nUpCount(int[]a,int n)
  {
    int x=0,psum=0;
    for(int m:a)
    {
      psum=psum+m;
      if(psum>n)
      {
        x+=1;
      }
    }
    
    
    return x;
     
  }
  public static void main(String args[])
  {
    int m[]={2,3,1,-6,8,-3,-1,2};
    int x=nUpCount(m,5);
    System.out.println(x);
  }
}