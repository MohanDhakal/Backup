class Length
{
  double length;
  
  void setLength(double l)
  {
    length=l;
  }
}
class Breadth extends Length
{
  double breadth;
  void setBreadth(double b)
  {
     breadth=b;
  }
  void area()
  {
    System.out.println("Area is : "+length*breadth);
  }
}
class SingleInh
{
  public static void main(String args[])
  {
    Breadth b=new Breadth();
    b.setLength(4.5);
    b.setBreadth(3.2);
    b.area();
  }
}
                    